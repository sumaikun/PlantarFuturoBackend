{
    "openapi": "3.0.0",
    "info": {
        "title": "API Usuarios",
        "version": "1.0"
    },
    "servers": [
        {
            "url": "http://localhost:8000"
        },
        {
            "url": "http://swagger.local"
        }
    ],
    "paths": {
        "/api/testExcel": {
            "post": {
                "summary": "Probar como llegan los datos de excel desde angular",
                "operationId": "App\\Http\\Controllers\\Api\\TestController::testExcel",
                "requestBody": {
                    "content": {
                        "application/json": {
                            "schema": {
                                "properties": {
                                    "id": {
                                        "type": "string"
                                    },
                                    "name": {
                                        "type": "string"
                                    }
                                },
                                "type": "object",
                                "example": {
                                    "id": 10,
                                    "name": "Jessica Smith"
                                }
                            }
                        }
                    }
                },
                "responses": {
                    "200": {
                        "description": "Muestra el resultado."
                    },
                    "400": {
                        "description": "Indica que no se mandar√≥n bien los parametros."
                    },
                    "405": {
                        "description": "Metodo no permitido."
                    },
                    "default": {
                        "description": "Ha ocurrido un error."
                    }
                }
            }
        },
        "/api/users": {
            "get": {
                "summary": "Mostrar usuarios",
                "operationId": "App\\Http\\Controllers\\Api\\UserController::index",
                "responses": {
                    "200": {
                        "description": "Mostrar todos los usuarios."
                    },
                    "default": {
                        "description": "Ha ocurrido un error."
                    }
                }
            }
        }
    }
}