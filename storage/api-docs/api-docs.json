                                    },
                                    "health_status": {
                                        "description": "Estado Sanitario",
                                        "type": "string",
                                        "format": "string",
                                        "example": "Bueno"
                                    },
                                    "origin": {
                                        "description": "Origen",
                                        "type": "string",
                                        "format": "string",
                                        "example": "Nativa"
                                    },
                                    "cup_density": {
                                        "description": "Densidad de copa",
                                        "type": "string",
                                        "format": "string",
                                        "example": "Clara"
                                    },
                                    "products": {
                                        "description": "Productos y Posible uso",
                                        "type": "string",
                                        "format": "string",
                                        "example": "Le√±a"
                                    },
                                    "margin": {
                                        "description": "Margen",
                                        "type": "string",
                                        "format": "string",
                                        "example": "Derecha"
                                    },
                                    "treatment": {
                                        "description": "Tipo de manejo",
                                        "type": "string",
                                        "format": "string",
                                        "example": "Tala"
                                    },
                                    "state": {
                                        "description": "Estado",
                                        "type": "string",
                                        "format": "string",
                                        "example": "Talado"
                                    },
                                    "resolution": {
                                        "description": "Resolucion",
                                        "type": "string",
                                        "format": "string",
                                        "example": "0366"
                                    },
                                    "general_image": {
                                        "description": "Foto general",
                                        "type": "string",
                                        "format": "string",
                                        "example": "null"
                                    },
                                    "before_image": {
                                        "description": "Foto del antes",
                                        "type": "string",
                                        "format": "string",
                                        "example": "null"
                                    },
                                    "after_image": {
                                        "description": "Foto del despues",
                                        "type": "string",
                                        "format": "string",
                                        "example": "null"
                                    },
                                    "start_treatment": {
                                        "description": "Fecha de inicio",
